    "@types/node": "^10.0.0",
    "@types/sinon": "^4.3.1",
    "@types/uuid": "^3.4.3",
    "mocha": "^5.1.1",
    "sinon": "^4.5.0",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-microsoft-contrib": "^5.0.3",
