    "build": "rm -rf dist/ && tsc && echo Done",
    "docs": "./node_modules/.bin/typedoc --mode file --name 'Payment Rails JavaScript SDK' --readme none --theme markdown --out docs --hideGenerator lib",
    "clean": "rm -rf dist/"
